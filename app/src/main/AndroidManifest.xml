<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mymiwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".Activities.NumbersActivity"
            android:exported="false"
            android:label="Numbers" />
        <activity
            android:name=".Activities.PhrasesActivity"
            android:exported="false"
            android:label="Phrases" />
        <activity
            android:name=".Activities.ColorsActivity"
            android:exported="false"
            android:label="Colors" />
        <activity
            android:name=".Activities.FamilyActivity"
            android:exported="false"
            android:label="Family Members" />
        <activity
            android:name=".Activities.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.MyMiwok">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

    </application>

</manifest>